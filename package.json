{
  "name": "vue-good-table",
  "version": "1.19.2",
  "description": "A simple, clean data table for VueJS (2.x) with essential features like sorting, column filtering, pagination etc",
  "main": "dist/vue-good-table.js",
  "scripts": {
    "bundle:min": "cross-env MINIFY=true webpack --progress --config build/webpack.bundle.conf.js",
    "bundle:full": "cross-env MINIFY=false webpack --progress --config build/webpack.bundle.conf.js",
    "bundle": "npm run bundle:min && npm run bundle:full",
    "dev": "node build/dev-server.js",
    "lint": "eslint --config=.eslintrc.js src/**/*.js src/**/*.vue test/unit/specs test/e2e/specs",
    "fix": "eslint --fix --config=.eslintrc.js src/**/*.js src/**/*.vue test/unit/specs test/e2e/specs",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "unit:watch": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --watch",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xaksis/vue-good-table"
  },
  "keywords": [
    "vue",
    "vuejs",
    "table",
    "datatable"
  ],
  "author": "Akshay Anand",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "date-fns": "^2.0.0-alpha.7",
    "lodash.clone": "^4.5.0",
    "lodash.foreach": "^4.5.0",
    "diacriticless": "1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chalk": "^2.3.1",
    "chromedriver": "^2.35.0",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.4.1",
    "cross-env": "^5.1.3",
    "cross-spawn": "^6.0.4",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.2",
